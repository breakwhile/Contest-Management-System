<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edu.xmut.MyContest.model.Events" table="events"
		dynamic-update="true">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="contestname" type="java.lang.String">
			<column name="contestname" length="64" />
		</property>
		<property name="scope" type="java.lang.String">
			<column name="scope" length="6" />
		</property>
		<property name="organizer" type="java.lang.String">
			<column name="organizer" length="64" />
		</property>
	</class>
</hibernate-mapping>

