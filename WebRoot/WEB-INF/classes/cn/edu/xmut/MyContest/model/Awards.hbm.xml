<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edu.xmut.MyContest.model.Awards" table="awards" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        
     	<property name="studentid" type="java.lang.Integer">
            <column name="studentid" />
        </property>
        
        <property name="teacherid" type="java.lang.Integer">
            <column name="teacherid" />
        </property>
        
        <property name="eventid" type="java.lang.Integer">
            <column name="eventid" />
        </property>       
        
        <property name="level" type="java.lang.String">
            <column name="level" length="32" />
        </property>         
        
        <property name="year" type="java.lang.String">
            <column name="year" length="32" />
        </property>
    </class>
</hibernate-mapping>

